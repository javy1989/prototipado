<div class="row">
  <div class="col s12">
    <div class="card-panel">
      <div class="card-content">
        <div class="row">
          <h5>Competidores Agregados</h5>
        </div>
        <div class="row">
          <div id="competidorInfo"></div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="row">
  <div class="col s12">
    <div class="card-panel">
      <div class="card-content">
        <form>
          <div class="row">
            <h5>Agregar Competidores</h5>
            <div class="input-field col s6">
              <input placeholder="" id="competidorName"
                     type="text"
                     name="competidorName"
                     class="validate">
              <label for="competidorName">Nombre del Competidor</label>
            </div>
          </div>

          <div class="row">
            <div class="input-field col s6">
              <input placeholder="" id="competidorUrl"
                     type="text"
                     name="competidorUrl"
                     class="validate">
              <label for="competidorUrl">URL</label>
            </div>
          </div>
          <div class="row">
            <a class="btn grey darken-3">
              AGREGAR COMPETIDOR
            </a>
          </div>
        </form>
      </div>
    </div>
  </div>

</div>
